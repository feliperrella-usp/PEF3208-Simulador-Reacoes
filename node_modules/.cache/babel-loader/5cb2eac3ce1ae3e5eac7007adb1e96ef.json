A,EAAA,CAAA,EAAA,EAAA;AACA,UAAA,IAAA,GAAA,MAAA,CAAA,MAAA,CAAA,SAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CAAA;AACA,WAAA,SAAA,CAAA,CAAA,EAAA,EAAA,GAAA,IAAA,CAAA,EAAA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AACA;;AACA,IAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,MAAA,CAAA,KAAA,EAAA,KAAA,MAAA;AACA,IAAA,MAAA,CAAA,qBAAA,CAAA,KAAA,MAAA;AACA,IAAA,MAAA,CAAA,QAAA,GAAA,KAAA,QAAA,EAAA,CA5GA,CA6GA;AACA;AACA;;AACA,IAAA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,MAAA;AACA,IAAA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,MAAA;AACA,GAhLA;AAiLA,EAAA,OAAA,EAAA;AACA,IAAA,cADA,0BACA,CADA,EACA,CADA,EACA,CADA,EACA;AACA,WAAA,MAAA,GAAA,CAAA;AACA,WAAA,MAAA,GAAA,CAAA;AACA,WAAA,CAAA,GAAA,CAAA,GAAA,CAAA,GAAA,EAAA,CAHA,CAIA;AACA;AACA,KAPA;AAQA,IAAA,kBARA,8BAQA,CARA,EAQA;AACA,UAAA,KAAA,OAAA,EAAA;AACA,aAAA,CAAA,GAAA,CAAA,GAAA,KAAA,MAAA;AACA,aAAA,EAAA,GAAA,KAAA,CAAA,GAAA,KAAA,CAAA,GAAA,GAAA,CAFA,CAGA;AACA;AACA;AACA,KAfA;AAgBA,IAAA,MAhBA,oBAgBA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,SAAA,CAAA,MAAA,EAAA,CAAA,GAAA,CAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,OAAA,GAAA,KAAA,SAAA,CAAA,CAAA,CAAA;AACA,YAAA,IAAA,GAAA,IAAA;;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,MAAA,CAAA,MAAA,EAAA,CAAA,GAAA,CAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,KAAA,MAAA,CAAA,CAAA,EAAA,EAAA,IAAA,OAAA,CAAA,EAAA,EAAA;AACA,YAAA,IAAA,GAAA,KAAA,MAAA,CAAA,CAAA,CAAA;AACA;AACA;AACA;;AAEA,YAAA,IAAA,KAAA,IAAA,EAAA,SAVA,CAWA;;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,SAAA,GACA,gBACA,KAAA,eAAA,CAAA,CAAA,EAAA,CADA,GAEA,MAFA,GAGA,KAAA,eAAA,CAAA,CAAA,EAAA,CAHA,GAIA,MALA,CAZA,CAkBA;AACA;;AACA,MAAA,MAAA,CAAA,qBAAA,CAAA,KAAA,MAAA;AACA,KAtCA;AAuCA,IAAA,WAvCA,uBAuCA,KAvCA,EAuCA,MAvCA,EAuCA,CAvCA,EAuCA,CAvCA,EAuCA,EAvCA,EAuCA,IAvCA,EAuCA;AACA,UAAA,KAAA,GAAA,QAAA,CAAA,aAAA,CAAA,KAAA,CAAA;AACA,MAAA,KAAA,CAAA,EAAA,GAAA,EAAA;AACA,MAAA,KAAA,CAAA,SAAA,GAAA,SAAA;AACA,MAAA,KAAA,CAAA,KAAA,CAAA,OAAA,GAAA,YAAA,MAAA,GAAA,YAAA,GAAA,KAAA,GAAA,KAAA;AACA,UAAA,OAAA,GAAA,QAAA,CAAA,cAAA,CAAA,IAAA,CAAA;AACA,MAAA,KAAA,CAAA,WAAA,CAAA,OAAA,EANA,CAMA;;AACA,WAAA,eAAA,CAAA,IAAA,CAAA;AAAA,QAAA,CAAA,EAAA,CAAA;AAAA,QAAA,CAAA,EAAA;AAAA,OAAA;AACA,MAAA,QAAA,CAAA,cAAA,CAAA,cAAA,EAAA,WAAA,CAAA,KAAA;AACA,KAhDA;AAiDA,IAAA,QAjDA,sBAiDA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA;AAnDA;AAjLA,CAAA","sourcesContent":["<template>\n  <div>\n    <div id=\"left\">\n      <v-card class=\"mx-auto card-render\" id=\"render\" outlined>\n        <div id=\"label-holder\">\n        </div>\n      </v-card>\n    </div>\n    <div id=\"right\" v-if=\"true\">\n      <v-card class=\"mx-auto card-info\" outlined>\n        <script\n          type=\"application/javascript\"\n          src=\"https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.2/MathJax.js?config=TeX-AMS_HTML\"\n        ></script>\n        <h1>Vigas em balanço</h1>\n        <div v-if=\"devTools\">\n          <h2>Mouse position: {{ absolute }}</h2>\n          <h2>inRange: {{ inRange }}</h2>\n          <h2>Largura da Barra: {{ size }} centimetros</h2>\n\n          <div v-if=\"inRange\">\n            <h2>X Clicado: {{ clickX }}</h2>\n            <h2>Y Clicado: {{ clickY }}</h2>\n            <h2>l: {{ l }}</h2>\n            <h2>Forca: {{ f }} N</h2>\n            <h2>Ya: {{ f }} N</h2>\n            <h2>Ma: {{ ma }} N</h2>\n          </div>\n        </div>\n        <div v-if=\"!devTools\">\n          <VueMathjax :formula=\"info\"></VueMathjax>\n          <VueMathjax :formula=\"sumFx\"></VueMathjax>\n          <VueMathjax :formula=\"sumFy\"></VueMathjax>\n          <VueMathjax v-if=\"f != 0\" :formula=\"f1\"></VueMathjax>\n          <VueMathjax v-if=\"f != 0\" :formula=\"f2\"></VueMathjax>\n        </div>\n      </v-card>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Matter from \"matter-js\";\nimport { VueMathjax } from \"vue-mathjax\";\n\nexport default {\n  name: \"App\",\n  components: {\n    VueMathjax,\n  },\n  computed: {\n    f1() {\n      //f \\cdot da - rb \\cdot l\n      return (\n        \"$$ {V}_a = F \\\\rightarrow {V}_a = \" +\n        Math.round(this.f * 100) / 100 +\n        \"N$$\"\n      );\n    },\n    f2() {\n      return (\n        \"$$ {M}_a = F \\\\cdot l \\\\rightarrow \" +\n        Math.round(this.f * 100) / 100 +\n        \" \\\\cdot\" +\n        Math.round((this.l / 100) * 100) / 100 +\n        \" =\" +\n        Math.round(this.ma * 100) / 100 +\n        \"$$\"\n      );\n    },\n    info() {\n      return (\n        \"$$ F = \" +\n        Math.round(this.f * 100) / 100 +\n        \" N ,\\\\ T = \" +\n        Math.round(this.sizeAvailable * 100) / 100 +\n        \"cm ,\\\\ l =\" +\n        Math.round(this.l * 100) / 100 +\n        \"cm $$\"\n      );\n    },\n  },\n  data() {\n    return {\n      clickX: \"\",\n      clickY: \"\",\n      absolute: \"\",\n      inRange: true,\n      l: 0,\n      size: 0,\n      sizeAvailable: 0,\n      ma: 0,\n      rb: 0,\n      f: 0,\n      devTools: false,\n      sumFx: \"$$\\\\sum {F}_x = 0$$\",\n      sumFy: \"$$\\\\sum {F}_y = 0 \\\\rightarrow {V}_a - F = 0 $$\",\n      sumMa: \"$$\\\\sum {M}_a = 0$$\",\n      bodiesDom: [],\n      bodies: [],\n      bodiesPositions: [\n        \n      ],\n      width: 0,\n      height: 0,\n    };\n  },\n  mounted() {\n    this.height = document.getElementById(\"render\").clientHeight;\n    this.width = document.getElementById(\"render\").clientWidth;\n    let engine = Matter.Engine.create();\n    let render = Matter.Render.create({\n      element: document.querySelector(\"#render\"),\n      engine: engine,\n      options: {\n        height: this.height,\n        width: this.width,\n        showDebug: this.devTools,\n      },\n    });\n\n    let ground = Matter.Bodies.rectangle(\n      this.width / 2,\n      this.height - this.height / 40,\n      this.width,\n      this.height / 20,\n      { isStatic: true }\n    );\n    let wall = Matter.Bodies.rectangle(\n      this.width / 40,\n      this.height / 2,\n      this.width / 20,\n      this.height,\n      { isStatic: true }\n    );\n    this.size = this.width * 0.6;\n    let boxA = Matter.Bodies.rectangle(\n      this.width * 0.3 + this.width / 20,\n      this.height / 2,\n      this.width * 0.6,\n      this.height / 20,\n      {\n        isStatic: true,\n        label: \"boxA\",\n        render: {\n          visible: true,\n        },\n      }\n    );\n    let constraint1 = Matter.Constraint.create({\n      bodyA: wall,\n      pointA: { x: this.width / 40, y: this.height / 40 },\n      bodyB: boxA,\n      pointB: { x: (-this.width * 0.6) / 2, y: this.height / 40 },\n    });\n\n    let constraint2 = Matter.Constraint.create({\n      bodyA: wall,\n      pointA: { x: this.width / 40, y: -this.height / 40 },\n      bodyB: boxA,\n      pointB: { x: (-this.width * 0.6) / 2, y: -this.height / 40 },\n    });\n\n    this.sizeAvailable = this.width * 0.6;\n    let mouse = Matter.Mouse.create(render.canvas);\n    this.absolute = mouse.absolute;\n\n    let mouseConstraint = Matter.MouseConstraint.create(engine, {\n      mouse: mouse,\n      constraint: {\n        render: { visible: true },\n      },\n    });\n\n    Matter.Events.on(mouseConstraint, \"startdrag\", (event) => {\n      if (event.body.label == \"boxA\") {\n        let position = event.mouse.absolute;\n        //console.log(position);\n        this.calculateSizes(position.x, position.y, this.width);\n      }\n    });\n\n    Matter.Events.on(mouseConstraint, \"enddrag\", (event) => {\n      console.log(event);\n      if (event.body.label == \"boxA\") {\n        let position = event.mouse.absolute;\n        //console.log(position);\n        this.calculateReactions(position.y);\n      }\n    });\n\n    render.mouse = mouse;\n    Matter.World.add(engine.world, [\n      boxA,\n      ground,\n      wall,\n      mouseConstraint,\n      constraint1,\n      constraint2,\n    ]);\n\n    //A\n    this.createLabel(this.width/20,this.height/20, 0,this.height / 2 - this.height / 40, \"A_point\", \"A\")\n    //Viga\n    this.createLabel(this.width/20,this.height/20, this.width*0.64 + this.width/20,this.height / 2 - this.height / 40, \"viga\", \"Viga\")\n\n    this.bodiesDom = document.querySelectorAll(\".label-2\");\n    this.bodies = [];\n    for (var i = 0, l = this.bodiesDom.length; i < l; i++) {\n      var body = Matter.Bodies.rectangle(0, 0, 0, 0);\n      this.bodiesDom[i].id = body.id;\n      this.bodies.push(body);\n    }\n    Matter.World.add(engine.world, this.bodies);\n    window.requestAnimationFrame(this.update);\n    window.onresize = this.onResize();\n    //box.render();\n    //Matter.Engine.update(engine);\n    //requestAnimationFrame(rerender);\n    Matter.Runner.run(engine);\n    Matter.Render.run(render);\n  },\n  methods: {\n    calculateSizes(x, y, w) {\n      this.clickX = x;\n      this.clickY = y;\n      this.l = x - w / 20;\n      //this.da = x - p1;\n      //this.db = this.sizeAvailable - this.da;\n    },\n    calculateReactions(y) {\n      if (this.inRange) {\n        this.f = y - this.clickY;\n        this.ma = (this.f * this.l) / 100;\n        //this.rb = (this.f * this.da) / this.sizeAvailable;\n        //this.ra = this.f - this.rb;\n      }\n    },\n    update() {\n      for (var i = 0, l = this.bodiesDom.length; i < l; i++) {\n        var bodyDom = this.bodiesDom[i];\n        var body = null;\n        for (var j = 0, k = this.bodies.length; j < k; j++) {\n          if (this.bodies[j].id == bodyDom.id) {\n            body = this.bodies[j];\n            break;\n          }\n        }\n\n        if (body === null) continue;\n        //console.log(this.bodiesPositions)\n        bodyDom.style.transform =\n          \"translate( \" +\n          this.bodiesPositions[i].x +\n          \"px, \" +\n          this.bodiesPositions[i].y +\n          \"px )\";\n        //bodyDom.style.transform += \"rotate( \" + body.angle + \"rad )\";\n      }\n      window.requestAnimationFrame(this.update);\n    },\n    createLabel(width, height,x,y, id, text) {\n      var label = document.createElement(\"div\");\n      label.id = id;\n      label.className = \"label-2\";\n      label.style.cssText = \"height:\" + height + \"px; width:\" + width + \"px;\"\n      var content = document.createTextNode(text);\n      label.appendChild(content); //adiciona o nó de texto à nova div criada\n      this.bodiesPositions.push({x:x, y:y})\n      document.getElementById(\"label-holder\").appendChild(label);\n    },\n    onResize(){\n      console.log(\"resized\")\n    }\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: \"Avenir\", Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2f67a0;\n  width: 100%;\n  height: 100%;\n}\n#left {\n  float: left;\n  width: 60%;\n  height: 100vh;\n}\n#right {\n  float: right;\n  width: 40%;\n  height: 100vh;\n}\n\n#MathJax_Message {\n  display: none;\n}\n\n.label {\n  justify-content: center;\n  align-items: center;\n  display: flex;\n  color: #fff;\n  z-index: 1;\n  position: absolute;\n}\n\n.card-info {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  width: 80%;\n  height: 100%;\n}\n\n.card-render {\n  position: absolute;\n  width: 80%;\n  height: 100%;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n</style>\n"],"sourceRoot":"src/components"}]}